<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.emr.slgi.doctor.dao.DoctorDAO">
  <select id="getDoctorList">
    SELECT
      uuid,
      name
    FROM
        TB_MEMBER
    WHERE
        member_role = 'R002'
  </select>
</mapper>
