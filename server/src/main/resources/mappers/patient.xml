<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.emr.slgi.patient.dao.ReservationDAO">

    <insert id="makeReservation">
        INSERT INTO
            TB_RESERVATION
        VALUES
            (
                UUID(),
                #{patientUuid}, -- 임시 데이터
                #{doctorUuid},
                'RS01', -- 임시 데이터
                #{date},
                #{symptom}
            )
    </insert>

</mapper>