<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.emr.slgi.reception.dao.ReceptionDAO">
  <insert id="acceptPatientByStaff">
    INSERT INTO
        TB_RECEPTION
    VALUES
        (
            #{patientUuid},
            #{doctorUuid},
            #{createdTime}
        )
  </insert>
</mapper>
